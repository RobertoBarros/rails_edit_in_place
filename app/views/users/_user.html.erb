<tr data-user-id='<%= user.id %>'>
  <th scope="row"><%= user.id %></th>
  <td><%= user.first_name %> <%= user.last_name %></td>
  <td><%= user.address %></td>
  <td><%= user.city %> / <%= user.state %></td>
  <td><%= user.postcode %></td>
  <td><%= user.phone %></td>
  <td><%= link_to edit_user_path(user), remote: true do %><i class="fas fa-edit"></i><% end %></td>
</tr>
